# misc
alias vim="nvim"
alias la="ls -lAhp"
alias whatport="lsof -i -P | grep"
alias vimgitdiff="vim \$(git add -N . && git diff master --name-only | xargs)"
alias mux="tmuxinator"

alias dotedit="~/.scripts/dotedit.sh start"

alias k="kubectl"

alias yq="~/.scripts/yaml2json.sh | jq"

alias vs="vimsplit"
